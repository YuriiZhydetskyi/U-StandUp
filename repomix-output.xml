                </div>
            </article>`;
        }

        function init() {
            initHeader('clubs');
            initFooter();

            const container = document.getElementById('clubs-container');
            container.innerHTML = clubs.map(renderClub).join('');
        }

        if (document.readyState === 'loading') {
            document.addEventListener('DOMContentLoaded', init);
        } else {
            init();
        }
    </script>
