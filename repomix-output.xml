}

.event-card__date-badge {
    position: absolute;
    top: 1rem;
    left: 1rem;
    background: var(--color-accent);
    color: var(--color-primary);
    padding: 0.5rem 0.75rem;
    border-radius: var(--radius-sm);
    text-align: center;
    box-shadow: var(--shadow-sm);
}

.event-card__date-badge .day {
    font-size: 1.5rem;
    font-weight: 700;
    line-height: 1;
    display: block;
}

.event-card__date-badge .month {
    font-size: 0.75rem;
    text-transform: uppercase;
    font-weight: 600;
}

.event-card__category-badge {
    position: absolute;
    top: 1rem;
    right: 1rem;
    background: rgba(0,0,0,0.7);
    color: white;
    padding: 0.25rem 0.75rem;
    border-radius: var(--radius-sm);
    font-size: 0.75rem;
    font-weight: 500;
}

.event-card__body {
    padding: 1.25rem;
}

.event-card__title {
    font-size: 1.15rem;
    font-weight: 700;
    margin: 0 0 0.5rem 0;
    color: var(--color-primary);
}

.event-card__meta {
    display: flex;
    flex-wrap: wrap;
    gap: 1rem;
    font-size: 0.9rem;
    color: var(--color-text-light);
    margin-bottom: 0.75rem;
}

.event-card__description {
    font-size: 0.9rem;
    line-height: 1.6;
    color: var(--color-text);
    margin-bottom: 1rem;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.event-card__actions {
    display: flex;
    gap: 0.75rem;
}

/* No image card */
.event-card--no-image .event-card__inner {
    display: flex;
    align-items: center;
    padding: 1rem;
    gap: 1rem;
}

.event-card--no-image .event-card__date-badge {
    position: static;
    flex-shrink: 0;
}

.event-card--no-image .event-card__body {
    padding: 0;
    flex: 1;
}

.event-card--no-image .event-card__description {
    display: none;
}

/* ===== READ MORE ===== */
.read-more-btn {
    background: none;
    border: none;
    color: var(--color-primary);
    font-weight: 500;
    cursor: pointer;
    padding: 0;
    font-size: 0.9rem;
}

.read-more-btn:hover {
    text-decoration: underline;
}

/* ===== LIGHTBOX ===== */
.lightbox {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.95);
    z-index: 10000;
    justify-content: center;
    align-items: center;
    cursor: pointer;
